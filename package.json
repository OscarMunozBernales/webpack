  "devDependencies": {
    "@babel/core": "7.8.0",
    "@babel/plugin-transform-runtime": "7.8.0",
    "@babel/preset-env": "7.8.2",
    "@babel/preset-react": "7.8.3",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.1",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
